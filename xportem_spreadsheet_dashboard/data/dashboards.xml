<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Create our own dashboard group -->
    <record id="spreadsheet_dashboard_group_procurement" model="spreadsheet.dashboard.group">
        <field name="name">Procurement</field>
        <field name="sequence">100</field>
    </record>

    <!-- Dashboard records with spreadsheet data -->
    <record id="spreadsheet_dashboard_procurement_daily" model="spreadsheet.dashboard">
        <field name="name">Seguimiento Diario de Procura</field>
        <field name="dashboard_group_id" ref="spreadsheet_dashboard_group_procurement"/>
        <field name="spreadsheet_binary_data" type="base64" file="xportem_spreadsheet_dashboard/data/files/procurement_daily_dashboard.json"/>
        <field name="group_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="sequence">100</field>
    </record>

    <record id="spreadsheet_dashboard_procurement_quotations" model="spreadsheet.dashboard">
        <field name="name">Análisis de Cotizaciones</field>
        <field name="dashboard_group_id" ref="spreadsheet_dashboard_group_procurement"/>
        <field name="spreadsheet_binary_data" type="base64" file="xportem_spreadsheet_dashboard/data/files/procurement_quotations_dashboard.json"/>
        <field name="group_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="sequence">200</field>
    </record>

    <record id="spreadsheet_dashboard_procurement_tracking" model="spreadsheet.dashboard">
        <field name="name">Tracking de Muestras y Envíos</field>
        <field name="dashboard_group_id" ref="spreadsheet_dashboard_group_procurement"/>
        <field name="spreadsheet_binary_data" type="base64" file="xportem_spreadsheet_dashboard/data/files/procurement_tracking_dashboard.json"/>
        <field name="group_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="sequence">300</field>
    </record>
</odoo>